      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>numberOfStudents</name>
      <display_name>Total Number of Students.</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
